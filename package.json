{
  "name": "koa2-starter",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "PORT=3000 NODE_ENV=production LOGGER_LEVEL=silly LOGGER_ENABLED=true SESSION_KEY=koasess node build/server.js",
    "dev": "gulp serve & PORT=3000 NODE_ENV=development LOGGER_LEVEL=silly LOGGER_ENABLED=true SESSION_KEY=koasess nodemon src/server.js",
    "build": "babel src --out-dir build --copy-files",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint src"
  },
  "author": "andy fenelon",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "handlebars": "^4.0.6",
    "joi": "^10.0.5",
    "koa": "^2.0.0",
    "koa-bodyparser": "next",
    "koa-compress": "next",
    "koa-convert": "^1.2.0",
    "koa-favicon": "2",
    "koa-passport": "next",
    "koa-redis": "^2.1.3",
    "koa-router": "^7.0.0",
    "koa-session": "^3.3.1",
    "koa-static": "3.0.0",
    "koa-views": "next",
    "lodash": "^4.17.2",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-stage-0": "^6.16.0",
    "babelify": "^7.3.0",
    "bootstrap-sass": "^3.3.7",
    "browser-sync": "^2.2.1",
    "browserify": "^13.1.1",
    "cli-color": "^1.1.0",
    "del": "^2.2.0",
    "eslint": "^3.12.2",
    "eslint-plugin-flowtype": "^2.29.1",
    "flow-bin": "^0.37.3",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.1",
    "gulp-babel": "^6.1.2",
    "gulp-browserify": "^0.5.1",
    "gulp-clean-css": "^2.3.2",
    "gulp-cssnano": "^2.0.0",
    "gulp-eslint": "^3.0.0",
    "gulp-file-cache": "^0.0.1",
    "gulp-filter": "^4.0.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-nodemon": "^2.2.1",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-uglify": "^2.0.0",
    "gulp-useref": "^3.0.0",
    "jquery": "^3.1.1",
    "main-bower-files": "^2.5.0",
    "roboto-fontface": "^0.6.0",
    "run-sequence": "^1.2.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "babel": {
    "presets": [
      "es2015-node6"
    ],
    "plugins": [
      "transform-async-to-generator",
      "transform-flow-strip-types"
    ],
    "ignore": [
      "src/public/styles",
      "src/public/scripts",
      "src/public/scripts/**",
      "build/public/scripts/**"
    ]
  }
}
